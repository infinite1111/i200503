<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <TextView
            android:paddingLeft="30dp"
            android:id="@+id/chat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Chat"
            android:layout_marginTop="48dp"
            android:textStyle="bold"
            android:textSize="27sp">

        </TextView>

        <TextView
            android:id="@+id/enter_code1"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginTop="100dp"
            android:paddingLeft="20dp"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:background="@drawable/rounded_white">
        </TextView>

        <ImageView
            android:paddingLeft="20dp"
            android:id="@+id/man1"
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:layout_marginTop="115dp"
            android:src="@drawable/man_one">
        </ImageView>

        <TextView
            android:paddingLeft="16dp"
            android:id="@+id/home1"
            android:layout_width="200dp"
            android:layout_toRightOf="@+id/man1"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Makenna Dias"
            android:layout_marginTop="120dp"
            android:textStyle="normal"
            android:textSize="22sp">
        </TextView>

        <TextView
            android:paddingLeft="16dp"
            android:id="@+id/message"
            android:layout_width="200dp"
            android:layout_toRightOf="@+id/man1"
            android:layout_height="wrap_content"
            android:textColor="@color/orange"
            android:text="1 new message"
            android:layout_marginTop="151dp"
            android:textStyle="normal"
            android:textSize="17sp">
        </TextView>

        <Button
            android:paddingLeft="20dp"
            android:layout_marginLeft="360dp"
            android:id="@+id/st_arrow"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="129dp"
            android:background="@drawable/horizontal_arrow">
        </Button>

        <TextView
            android:id="@+id/enter_code2"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginTop="200dp"
            android:paddingLeft="20dp"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:background="@drawable/rounded_white">
        </TextView>

        <ImageView
            android:paddingLeft="20dp"
            android:id="@+id/man2"
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:layout_marginTop="215dp"
            android:src="@drawable/man_two">
        </ImageView>

        <TextView
            android:paddingLeft="16dp"
            android:id="@+id/home2"
            android:layout_width="200dp"
            android:layout_toRightOf="@+id/man2"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Miracle Philips"
            android:layout_marginTop="220dp"
            android:textStyle="normal"
            android:textSize="22sp">
        </TextView>

        <TextView
            android:paddingLeft="16dp"
            android:id="@+id/message1"
            android:layout_width="200dp"
            android:layout_toRightOf="@+id/man1"
            android:layout_height="wrap_content"
            android:textColor="@color/gray"
            android:text="No new messages"
            android:layout_marginTop="251dp"
            android:textStyle="normal"
            android:textSize="15sp">
        </TextView>

        <Button
            android:paddingLeft="20dp"
            android:layout_marginLeft="360dp"
            android:id="@+id/st_arrow2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="229dp"
            android:background="@drawable/horizontal_arrow">
        </Button>

        <TextView
            android:id="@+id/enter_code3"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginTop="300dp"
            android:paddingLeft="20dp"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:background="@drawable/rounded_white">
        </TextView>

        <ImageView
            android:paddingLeft="20dp"
            android:id="@+id/man3"
            android:layout_width="100dp"
            android:layout_height="70dp"
            android:layout_marginTop="315dp"
            android:src="@drawable/man_three">
        </ImageView>

        <TextView
            android:paddingLeft="16dp"
            android:id="@+id/home3"
            android:layout_width="200dp"
            android:layout_toRightOf="@+id/man3"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Zaire Gouse"
            android:layout_marginTop="320dp"
            android:textStyle="normal"
            android:textSize="22sp">
        </TextView>

        <TextView
            android:paddingLeft="16dp"
            android:id="@+id/message2"
            android:layout_width="200dp"
            android:layout_toRightOf="@+id/man1"
            android:layout_height="wrap_content"
            android:textColor="@color/gray"
            android:text="No new messages"
            android:layout_marginTop="351dp"
            android:textStyle="normal"
            android:textSize="15sp">
        </TextView>

        <Button
            android:paddingLeft="20dp"
            android:layout_marginLeft="360dp"
            android:id="@+id/st_arrow3"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="329dp"
            android:background="@drawable/horizontal_arrow">
        </Button>

        <TextView

            android:id="@+id/box"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_alignTop="@+id/man3"
            android:layout_marginTop="440dp"
            android:paddingLeft="20dp"
            android:gravity="center_vertical"
            android:textSize="20sp"
            android:background="@drawable/rounded_white"
            >
        </TextView>










        <ImageView
            android:paddingLeft="30dp"
            android:id="@+id/home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/man3"
            android:src="@drawable/home"
            android:layout_marginTop="455dp"
            android:layout_marginRight="20dp"

            />

        <TextView
            android:paddingLeft="30dp"
            android:id="@+id/home4"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Home"
            android:layout_alignTop="@+id/home"
            android:layout_marginTop="40dp"
            android:textStyle="normal"
            android:textSize="15sp">
        </TextView>

        <ImageView

            android:id="@+id/search1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/home4"
            android:layout_alignTop="@+id/man3"
            android:src="@drawable/search_big"
            android:layout_marginTop="455dp"
            />

        <TextView
            android:id="@+id/search2"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:text="Search"
            android:textColor="@color/black"
            android:layout_alignTop="@+id/search1"
            android:layout_toRightOf="@+id/home4"
            android:layout_marginTop="40dp"
            android:textStyle="normal"
            android:textSize="15sp">
        </TextView>

        <ImageView

            android:id="@+id/add_circle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/home4"
            android:layout_marginLeft="70dp"
            android:layout_alignTop="@+id/man3"
            android:src="@drawable/add_circle"
            android:layout_marginTop="460dp"
            app:tint="@color/orange"

            />


        <ImageView

            android:id="@+id/chat3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/add_circle"
            android:layout_marginLeft="25dp"
            android:layout_alignTop="@+id/man3"
            android:src="@drawable/chat"
            android:layout_marginTop="455dp"
            app:tint="@color/orange"

            />

        <TextView
            android:id="@+id/chat2"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:textColor="@color/orange"
            android:text="Chat"
            android:layout_alignTop="@+id/search1"
            android:layout_toRightOf="@+id/search2"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="45dp"
            android:textStyle="normal"
            android:textSize="15sp">
        </TextView>


        <ImageView

            android:id="@+id/profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/chat3"
            android:layout_marginLeft="40dp"
            android:layout_alignTop="@+id/man3"
            android:src="@drawable/profile"
            android:layout_marginTop="455dp"

            />

        <TextView
            android:id="@+id/profile2"
            android:layout_marginLeft="40dp"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Profile"
            android:layout_alignTop="@+id/profile"
            android:layout_toRightOf="@+id/chat3"
            android:layout_marginTop="40dp"
            android:textStyle="normal"
            android:textSize="15sp">
        </TextView>






    </RelativeLayout>
</LinearLayout>